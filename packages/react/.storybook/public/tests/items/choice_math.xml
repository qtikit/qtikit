<?xml version="1.0"?>
<assessmentItem adaptive="false" identifier="choice" xmlns="http://www.imsglobal.org/xsd/imsqti_v2p2" xmlns:m="http://www.w3.org/1998/Math/MathML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p2  http://www.imsglobal.org/xsd/qti/qtiv2p2/imsqti_v2p2p3.xsd">
	<responseDeclaration baseType="identifier" cardinality="single" identifier="RESPONSE1">
		<correctResponse>
			<value>
				A
			</value>
		</correctResponse>
	</responseDeclaration>
	<outcomeDeclaration baseType="float" cardinality="single" identifier="SCORE">
		<defaultValue>
			<value>
				0
			</value>
		</defaultValue>
	</outcomeDeclaration>
	<itemBody>
		<choiceInteraction maxChoices="1" responseIdentifier="RESPONSE1" shuffle="false" xmlns:default="http://www.w3.org/1998/Math/MathML">
			<prompt>
				<p>
					Which number sentence could be used to make 81?
				</p>
			</prompt>
			<simpleChoice fixed="true" identifier="A" xmlns:default="http://www.w3.org/1998/Math/MathML">
				<div xmlns:default="http://www.w3.org/1998/Math/MathML">
					<p>
						<m:math xmlns="http://www.w3.org/1998/Math/MathML">
							<m:mn>
								81
							</m:mn>
							<m:mo>
								+
							</m:mo>
							<m:mn>
								9
							</m:mn>
							<m:mo>
								=
							</m:mo>
							<m:menclose notation="box">
								<m:mtext>
									?
								</m:mtext>
							</m:menclose>
						</m:math>
					</p>
				</div>
			</simpleChoice>
			<simpleChoice fixed="true" identifier="B" xmlns:default="http://www.w3.org/1998/Math/MathML">
				<div xmlns:default="http://www.w3.org/1998/Math/MathML">
					<p>
						<m:math xmlns="http://www.w3.org/1998/Math/MathML">
							<m:mrow>
								<m:mn>15</m:mn>
								<m:mo>&#x2212;</m:mo>
								<m:mn>3</m:mn>
								<m:mo>=</m:mo>
								<m:mn>12</m:mn>
								<m:mspace linebreak="newline" />
								<m:mn>12</m:mn>
								<m:mo>&#x2212;</m:mo>
								<m:mn>3</m:mn>
								<m:mo>=</m:mo>
								<m:mn>9</m:mn>
								<m:mspace linebreak="newline" />
								<m:mn>9</m:mn>
								<m:mo>&#x2212;</m:mo>
								<m:mn>3</m:mn>
								<m:mo>=</m:mo>
								<m:mn>6</m:mn>
								<m:mspace linebreak="newline" />
								<m:mn>6</m:mn>
								<m:mo>&#x2212;</m:mo>
								<m:mn>3</m:mn>
								<m:mo>=</m:mo>
								<m:mn>3</m:mn>
								<m:mspace linebreak="newline" />
								<m:mn>3</m:mn>
								<m:mo>&#x2212;</m:mo>
								<m:mn>3</m:mn>
								<m:mo>=</m:mo>
								<m:mn>0</m:mn>
							</m:mrow>
						</m:math>
					</p>
				</div>
			</simpleChoice>
			<simpleChoice fixed="true" identifier="C" xmlns:default="http://www.w3.org/1998/Math/MathML">
				<div xmlns:default="http://www.w3.org/1998/Math/MathML">
					<p>
						<m:math xmlns="http://www.w3.org/1998/Math/MathML">
							<m:mrow>
								<m:msup>
									<m:mrow>
										<m:mi>A</m:mi>
									</m:mrow>
									<m:mrow>
										<m:mo>&#x2032;</m:mo>
									</m:mrow>
								</m:msup>
								<m:mrow>
									<m:mo>(</m:mo>
									<m:mo>&#x2212;</m:mo>
									<m:mn>4</m:mn>
									<m:mo>,</m:mo>
									<m:mn>2</m:mn>
									<m:mo>)</m:mo>
								</m:mrow>
								<m:mo>,</m:mo>
								<m:msup>
									<m:mrow>
										<m:mi>B</m:mi>
									</m:mrow>
									<m:mrow>
										<m:mo>&#x2032;</m:mo>
									</m:mrow>
								</m:msup>
								<m:mrow>
									<m:mo>(</m:mo>
									<m:mn>1</m:mn>
									<m:mo>,</m:mo>
									<m:mn>4</m:mn>
									<m:mo>)</m:mo>
								</m:mrow>
								<m:mo>,</m:mo>
								<m:msup>
									<m:mrow>
										<m:mi>C</m:mi>
									</m:mrow>
									<m:mrow>
										<m:mo>&#x2032;</m:mo>
									</m:mrow>
								</m:msup>
								<m:mrow>
									<m:mo>(</m:mo>
									<m:mn>4</m:mn>
									<m:mo>,</m:mo>
									<m:mo>&#x2212;</m:mo>
									<m:mn>2</m:mn>
									<m:mo>)</m:mo>
								</m:mrow>
								<m:mo>,</m:mo>
								<m:msup>
									<m:mrow>
										<m:mi>D</m:mi>
									</m:mrow>
									<m:mrow>
										<m:mo>&#x2032;</m:mo>
									</m:mrow>
								</m:msup>
								<m:mrow>
									<m:mo>(</m:mo>
									<m:mo>&#x2212;</m:mo>
									<m:mn>2</m:mn>
									<m:mo>,</m:mo>
									<m:mo>&#x2212;</m:mo>
									<m:mn>1</m:mn>
									<m:mo>)</m:mo>
								</m:mrow>
							</m:mrow>
						</m:math>
					</p>
				</div>
			</simpleChoice>
			<simpleChoice fixed="true" identifier="D" xmlns:default="http://www.w3.org/1998/Math/MathML">
				<div xmlns:default="http://www.w3.org/1998/Math/MathML">
					<p>
						<m:math xmlns="http://www.w3.org/1998/Math/MathML">
							<m:mrow>
								<m:mn>2</m:mn>
								<m:mi>x</m:mi>
								<m:mo>+</m:mo>
								<m:mn>1</m:mn>
								<m:mo>+</m:mo>
								<m:mfrac>
									<m:mrow>
										<m:mo>&#x2212;</m:mo>
										<m:mn>6</m:mn>
									</m:mrow>
									<m:mrow>
										<m:mi>x</m:mi>
										<m:mo>&#x2212;</m:mo>
										<m:mn>1</m:mn>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:math>
					</p>
				</div>
			</simpleChoice>
		</choiceInteraction>
	</itemBody>
	<responseProcessing>
		<setOutcomeValue identifier="SCORE1">
			<baseValue baseType="float">1</baseValue>
		</setOutcomeValue>
	</responseProcessing>
</assessmentItem>